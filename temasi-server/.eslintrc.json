{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["google"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "require-jsdoc": "off",
    "new-cap":"off",
    "max-len":"off",
    "valid-jsdoc":"off",
    "camelcase": "off",
    "no-throw-literal":"off"
  }
}
